                                            <li class="wizard"><a href="#tab_4" data-toggle="tab">Ürün Sepeti</a></li>
                                            <li class="wizard"><a href="#tab_5" data-toggle="tab">Ödeme Bilgileri</a></li>
                                        </ul>
                                            <div class="tab-content">
                                                <div class="tab-pane active" id="tab_1">
                                                    <div id="divCompetent" class="row">
                                                        <div class="col-md-10 col-md-offset-1">
                                                            <div class="box-body">
                                                                <div class="form-group">
                                                                    <label>Yetkili</label>
                                                                       <%if(operation=='update'){
                                                                           var competentInfoIndex=-1;
                                                                        }%>
                                                                        <select id="slctCompetent" name="competentId" type="text"  class="form-control">
                                                                            <option value="Seçiniz" selected disabled>Yetkili Seçiniz</option>
                                                                             <%if(operation=='update'){%>
                                                                               <%for(var i=0;i<session.customer.competentInfo.length;i++){%>
                                                                               <option <%if(session.customer.competentInfo[i]._id==offer.competentInfo.competentId){ competentInfoIndex=i;%> selected <%}%>value="<%=session.customer.competentInfo[i]._id%>" ><%=session.customer.competentInfo[i].name%></option>
                                                                               <%}%>
                                                                            <%}%>
                                                                        </select>
                                                                </div>
                                                                <div class="form-group">
                                                                    <label >Yetkili Adı</label>
                                                                    <input id="inpCompetentInfoName" type="text"disabled class="form-control compName" placeholder="Yetkili Adı Giriniz" <%if(operation=='update' && competentInfoIndex != -1){%>value="<%=session.customer.competentInfo[competentInfoIndex].name%>"<%}%>>
                                                                </div>
                                                                <div class="form-group">
                                                                    <label >Görev</label>
                                                                    <input id="inpCompetentInfoTask" type="text" disabled class="form-control compTask"   placeholder="Görevi Giriniz" <%if(operation=='update' && competentInfoIndex != -1){%>value="<%=session.customer.competentInfo[competentInfoIndex].task%>"<%}%>>
                                                                </div>
                                                                <div class="form-group">
                                                                    <label>GSM</label>
                                                                    <input id="inpCompetentInfoGsm"  type="text" disabled class="form-control compGsm" data-inputmask='"mask": "(999) 999-9999"' data-mask  placeholder="Telefon Giriniz" <%if(operation=='update' && competentInfoIndex != -1){%>value="<%=session.customer.competentInfo[competentInfoIndex].gsm%>"<%}%>>
                                                                </div>
                                                                <div class="form-group">
                                                                    <label >E-mail</label>
                                                                    <input id="inpCompetentInfoEmail" type="email" disabled class="form-control compEmail"  placeholder="E-mail Giriniz" <%if(operation=='update' && competentInfoIndex != -1){%>value="<%=session.customer.competentInfo[competentInfoIndex].email%>"<%}%>>
                                                                </div>
                                                            </div><!-- /.box-body -->
                                                        </div>
                                                    </div>
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <div style="text-align:center;">
                                                                <a href="#tab_2" data-toggle="tab" class="btn btn-primary next"> İleri</a>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div><!-- /.tab-pane -->
                                                <div class="tab-pane" id="tab_2">
                                                    <div id="divContact" class="row">
                                                        <div class="col-md-10 col-md-offset-1">
                                                            <div class="box-body">
                                                                <div class="form-group">
                                                                    <label >Adres</label>
                                                                    <textarea id="inpContactInfoAddress" type="text" class="form-control contAddress" disabled rows="2" style="resize:vertical;" <%if(operation=='update'){%>value="<%=session.customer.contactInfo.address%>"<%}%>><%if(operation=='update'){%><%=session.customer.contactInfo.address%><%}%></textarea>
                                                                </div>
                                                                <div class="row">
                                                                    <div class="col-md-6">
                                                                        <div class="form-group">
                                                                            <label >İl</label>
                                                                            <input id="inpContactInfoCity" type="text" disabled class="form-control" <%if(operation=='update'){%>value="<%=session.customer.contactInfo.city%>"<%}%>>
