                                                                        <td class="col-md-11">
                                                                            <span class="pull-right">Genel İskonto</span>
                                                                        </td>
                                                                        <td class="col-md-1">
                                                                            <input id="generalDiscount" type="text" class="form-control" name="generalDiscount"<%if(operation == "update"){%> value="<%=offer.cost.generalDiscount%>"<%}else{%><%}%>>
                                                                        </td>
                                                                    </tr>
                                                                    <tr>
                                                                        <td class="col-md-11">
                                                                            <span class="pull-right">Tutar</span>
                                                                        </td>
                                                                        <td class="col-md-1">
                                                                            <input  type="text" class=" form-control" name="sum" id="productSum" <%if(operation == "update"){%> value="<%=offer.cost.sum%>"<%}%>>
                                                                        </td>
                                                                    </tr>
                                                                    <tr>
                                                                        <td class="col-md-11">
                                                                            <span class="pull-right">K.D.V</span>
                                                                        </td>
                                                                        <td class="col-md-1">
                                                                            <input type="text"  class="form-control" name="productKDV" id="productKDV" <%if(operation == "update"){%> value="<%=offer.cost.kdv%>"<%}%>>
                                                                        </td>
                                                                    </tr>
                                                                    <tr>
