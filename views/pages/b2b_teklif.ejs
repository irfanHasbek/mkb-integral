                                                                    <select name="" id="selectProduct" class="form-control">
                                                                        <option value="Seçiniz" disabled selected>Ürün Seçiniz</option>
                                                                    </select>
                                                                </div>
                                                                <div class="form-group">
                                                                    <div class="row">
                                                                        <div class="col-md-12">
                                                                            <label for="" style="margin-bottom:15px;">Girmek İstediğiniz Değeri Seçiniz</label>
                                                                            <div class="row">
                                                                                <div class="col-md-12 no-padding">
                                                                                    <div class="col-md-3">
                                                                                        <a id="btnRect" class="btn btn-default btn-flat btn-block">Dikdörtgen</a>                                                                                    
                                                                                    </div>
                                                                                    <div class="col-md-3">
                                                                                        <a id="btnCir" class="btn-block btn btn-default btn-flat" >Dairesel</a>
                                                                                    </div>
                                                                                    <div class="col-md-3">
                                                                                        <a id="btnOther" class="btn-block btn btn-default btn-flat" >Diger</a>
                                                                                    </div>
                                                                                    <div class="col-md-3">
                                                                                        <select id="slctUnit" class="form-control">
                                                                                            <option selected disabled value="">Birim Seciniz</option>
                                                                                            <%for(var i = 0; i < units.length; i++){%>
                                                                                               <option value="<%=units[i].unit%>"><%=units[i].unit%></option> 
                                                                                            <%}%>
                                                                                        </select>
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                    <br>
                                                                   <input id="inpSizeType" style="display:none;">
                                                                   <input id="inpBasket" name="basket" style="display:none;" <%if(operation=="update"){%>value="<%=JSON.stringify(offer.basket)%>"<%}%>> 
                                                                    <div id="divRec" class="row" style="display:none;">
                                                                        <div class="col-md-12 no-padding">
                                                                            <div class="col-md-3">
                                                                                <input id="inpEn" type="number" value="0" class="form-control" placeholder="En Giriniz">
                                                                            </div>
                                                                            <div class="col-md-3">
                                                                                <input id="inpYukseklik" type="number" value="0" class="form-control" placeholder="Yükselik Giriniz">
                                                                            </div>
                                                                            <div class="col-md-3">
                                                                                <input id="inpBoy" type="number" value="0" class="form-control" placeholder="Boy Giriniz">
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                    <div id="divCir" class="row" style="display:none;">
                                                                        <div class="col-md-12 no-padding">
                                                                            <div class="col-md-3">
                                                                                <input id="inpCap" type="number" value="0" class="form-control" placeholder="Çap Giriniz">
                                                                            </div>
                                                                            <div class="col-md-3">
                                                                                <input id="inpUzunluk" type="number" value="0" class="form-control" placeholder="Boy Giriniz">
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                    <div id="divOther" class="row" style="display:none;">
                                                                        <div class="col-md-12 no-padding">
                                                                            <div class="col-md-3">
                                                                                <input id="inpAdet" type="number" value="0" class="form-control" placeholder="Adet Giriniz">
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="row">
                                                                    <div class="col-md-2">
                                                                        <div class="form-group">
                                                                            <label for="">Montaj Şekli</label>
                                                                            <select id="montageType" class="form-control">
                                                                                <option style="padding-left:0px;" data='0' >Seçiniz</option>
                                                                            </select>
                                                                        </div>
                                                                    </div>
                                                                    <div class="col-md-2">
                                                                        <div class="form-group">
                                                                            <label for="">Kaplama Şekli</label>
                                                                            <select id="coverType" class="form-control">
                                                                                <option style="padding-left:0px;" data='0' >Seçiniz</option>
                                                                                   <%for(var i = 0; i < coverTypes.length; i++){%>
                                                                                    <option data="<%=coverTypes[i].cost%>" value="<%=coverTypes[i].coverType%>"><%=coverTypes[i].coverType%></option>
                                                                                  <%}%>
                                                                            </select>
                                                                        </div>
                                                                    </div>
