                                                                            <select id="setMechanism" class="form-control">
                                                                                 <option style="padding-left:0px;" data='0' >Seçiniz</option>
                                                                            </select>
                                                                        </div>
                                                                    </div>
                                                                    <div class="col-md-2">
                                                                        <div class="form-group">
                                                                            <label for="">Aksesuar</label>
                                                                            <select id="accessory" class="form-control">
                                                                                <option style="padding-left:0px;" data='0' >Seçiniz</option>
                                                                            </select>
                                                                        </div>
                                                                    </div>
                                                                    <div class="col-md-2">
                                                                        <div class="form-group">
                                                                            <label for="">Kasa Tipi</label>
                                                                            <select  id="bodyType" class="form-control">
                                                                                <option style="padding-left:0px;" data='0' >Seçiniz</option>
                                                                            </select>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                <div class="form-group">
                                                                    <label >Miktar</label>
                                                                    <input type="text" class="form-control" id="amount" placeholder="Miktar Giriniz" value='0'>
