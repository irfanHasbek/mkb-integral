                                                                        <th>Ürün Adi</th>
                                                                        <th>Ölçüler </th>
                                                                        <th>Montaj Şekli</th>
                                                                        <th>Kaplama Şekli</th>
                                                                        <th>Ayar Mekanizması</th>
                                                                        <th>Aksesuar</th>
                                                                        <th>Kasa Tipi</th>
                                                                        <th style="width:40px">Miktar</th>
                                                                        <th>Liste Fiyati</th>
                                                                        <th colspan="2">Iskontolu Birim Fiyati</th>
                                                                        <th style="width:40px">Tutar</th>
                                                                        <th style="width:40px"> </th>
                                                                    </tr>
                                                                    <tbody id="tableBasket">
                                                                        <%if(operation=='update'){%>
                                                                        <%for(var i=0;i<offer.basket.length;i++){%>
                                                                            <tr id="<%=offer.basket[i]._id%>">
                                                                            <td><%=(i+1)%></td>
                                                                            <td><%=offer.basket[i].productCodes%></td>
                                                                            <td><%=offer.basket[i].productGroup%> - <%=offer.basket[i].productName%></td>
                                                                            <%if(offer.basket[i].productSizeType=='Dikdörtgen'){%>
                                                                            <td><%=offer.basket[i].productSizeWidthOrDiameter%> x <%=offer.basket[i].productSizeHeight%> x <%=offer.basket[i].productSizeLength%><font size="1" color="blue">  (genişlik*yükseklik*uzunluk)</font></td>
                                                                            <%}else{%>
                                                                            <td><%=offer.basket[i].productSizeWidthOrDiameter%> x <%=offer.basket[i].productSizeLength%> <font size="1" color="blue">  (çap*uzunluk)</font></td>
                                                                            <%}%>
                                                                            <td><%=offer.basket[i].montageType%></td>
                                                                            <td><%=offer.basket[i].coverType%></td>
                                                                            <td><%=offer.basket[i].setMechanism%></td>
                                                                            <td><%=offer.basket[i].accessory%></td>
                                                                            <td><%=offer.basket[i].bodyType%></td>
                                                                            <td><%=offer.basket[i].amount%> <%=offer.basket[i].unit%></td>
                                                                            <td><%=offer.basket[i].productListPrice%></td>
                                                                            <td style="width:40px">% <%=offer.basket[i].lineDiscount%></td>
                                                                            <td style="width:80px"><%=offer.basket[i].productPriceWithDiscount%></td>
                                                                            <td><%=offer.basket[i].productPrice%></td>
                                                                            <td><button class="btn btn-danger btn-flat btn-sm remove"><i class="fa fa-trash-o"></i></button></td>
                                                                        </tr>
                                                                        <%}%>
                                                                        <%}%>
                                                                    </tbody>


                                                                </table>
                                                                <table class="table table-condensed table-bordered table-responsive" style="width:100%;">
